      "source": [
        "def equation_generation(operator:str, quantities:str):\n",
        "  equation =\"\"\n",
        "  for i in quantities:\n",
        "    equation = equation+i+operator\n",
        "\n",
        "  return equation[:-1]\n",
        "\n"
      ]
    },
